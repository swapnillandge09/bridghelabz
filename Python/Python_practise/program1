list = [1,5,8,3,8,6,6,364,64,33,67]
odd = []
sum_odd = 0
count_odd = 0
for i in list:
    if i % 2 != 0:
        odd.append(i)
        sum_odd += i
        count_odd += 1
if count_odd > 0:
    average = sum_odd/count_odd
else:
    average = 0

print(f"odd Number {odd}")
print(f"average number {average}")

